        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.godfunc.mapper.DictMapper">

    <resultMap id="BaseResultMap" type="com.godfunc.entity.Dict">
